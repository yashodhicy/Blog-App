<% if user_signed_in? %>
  <%= link_to 'Sign Out', destroy_user_session_path, data: { turbo_method: :delete } %>
<% else %>
  <%= link_to 'Login', new_user_session_path %>
<% end %>

<%"No users" if @users.empty?%>
<% @users.each do |user| %>
    <a href="<%= user_path(user) %>"><%= render user%></a>
    <br>
<% end %>
